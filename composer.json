{
    "name": "devsohail/laravel-model-expires",
    "description": "A package to assign expiration dates to Eloquent models",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "date",
        "expire",
        "expires",
        "expiration",
        "expirable"
    ],
    "homepage": "https://github.com/devsohail/laravel-model-expires",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "illuminate/database": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "doctrine/dbal": "^2.10 || ^3.0",
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^6.24 || ^7.0",
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvdnbrk\\EloquentExpirable\\ExpirableServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\EloquentExpirable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\EloquentExpirable\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
